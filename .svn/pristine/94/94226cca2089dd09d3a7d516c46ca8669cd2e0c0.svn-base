package com.vsoftcorp.kls.data;

/*
 *  @author a1565
 */

public class PurposeData {
	
	
	private Integer id;
	private String name;
	private String activityRequired;
	private String subPurposeRequired;
	private String masterAppRequired;
	private String remarks;
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getActivityRequired() {
		return activityRequired;
	}
	public void setActivityRequired(String activityRequired) {
		this.activityRequired = activityRequired;
	}
	
	public String getSubPurposeRequired() {
		return subPurposeRequired;
	}
	public void setSubPurposeRequired(String subPurposeRequired) {
		this.subPurposeRequired = subPurposeRequired;
	}
	public String getMasterAppRequired() {
		return masterAppRequired;
	}
	public void setMasterAppRequired(String masterAppRequired) {
		this.masterAppRequired = masterAppRequired;
	}	
	public String getRemarks() {
		return remarks;
	}
	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}
}
