package com.vsoftcorp.kls.data;

import java.math.BigDecimal;

/***
 * @author a1565
 * 
 */
public class PacsLoanEntryActivityData {

	private Long activityId;
	private Integer noOfUnit;
	private BigDecimal requestedAmount;
	private BigDecimal amountAsPerUnitCost;
	private String activityName;
	private String unitCost;
	private String unitName;

	public Long getActivityId() {
		return activityId;
	}

	public void setActivityId(Long activityId) {
		this.activityId = activityId;
	}

	public Integer getNoOfUnit() {
		return noOfUnit;
	}

	public void setNoOfUnit(Integer noOfUnit) {
		this.noOfUnit = noOfUnit;
	}

	public BigDecimal getRequestedAmount() {
		return requestedAmount;
	}

	public void setRequestedAmount(BigDecimal requestedAmount) {
		this.requestedAmount = requestedAmount;
	}

	public BigDecimal getAmountAsPerUnitCost() {
		return amountAsPerUnitCost;
	}

	public void setAmountAsPerUnitCost(BigDecimal amountAsPerUnitCost) {
		this.amountAsPerUnitCost = amountAsPerUnitCost;
	}

	public String getActivityName() {
		return activityName;
	}

	public void setActivityName(String activityName) {
		this.activityName = activityName;
	}

	public String getUnitCost() {
		return unitCost;
	}

	public void setUnitCost(String unitCost) {
		this.unitCost = unitCost;
	}

	public String getUnitName() {
		return unitName;
	}

	public void setUnitName(String unitName) {
		this.unitName = unitName;
	}

}
