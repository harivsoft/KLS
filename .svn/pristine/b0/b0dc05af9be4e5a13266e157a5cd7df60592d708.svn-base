package com.vsoftcorp.kls.report.service.exception;
/**
 * This class throws custom runtime exception.
 * @author a9152
 *
 */
public class KlsReportRuntimeException extends RuntimeException {

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	public KlsReportRuntimeException() {
	}

	public KlsReportRuntimeException(String message) {
		super(message);
	}

	public KlsReportRuntimeException(String message, Throwable excp) {
		super(message, excp);
	}
}
